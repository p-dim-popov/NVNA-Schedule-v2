{{#each days}}
    <ol>
        <span class="dayOfWeek">{{this.day}}, {{this.date}}</span>
        {{#each this.classes}}
            <li class="subject">
                {{this.name}}
                <ul class="info">
                    <li>{{this.time}}</li>
                    <li>{{this.groups}}</li>
                    <li>{{this.lecturer}}</li>
                </ul>
            </li>
        {{/each}}
    </ol>
{{/each}}